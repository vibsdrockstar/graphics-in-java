Color temp;
	for(int i=0;i<rows;i++)
	{
	if(i%2==0)
	{
		temp =col1;
	}
	else
	{
		temp=col2;
	}
	for(int j=0;j<columns;j++)
	{
		JPanel panel=new JPanel();
		panel.setBackground(temp);
		if(temp.equals(col1)) 
		{
			temp= col2;
			
			
			
		}
		else
		{
		temp= col1;
				
	}
		add(panel);
		
	}
}
}
}
